        <mxCell id="w9D8JjucIjg7rp5rkJSN-65" value="Elasticsearch" style="edgeLabel;html=1;align=center;verticalAlign=middle;resizable=0;points=[];" vertex="1" connectable="0" parent="w9D8JjucIjg7rp5rkJSN-64">
          <mxGeometry x="0.0059" y="4" relative="1" as="geometry">
            <mxPoint x="-4" y="-15" as="offset" />
